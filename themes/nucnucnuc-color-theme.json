// >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>  TOKEN COLOURS 


  "tokenColors": [

    
    {
      "name": "Modules",
      "scope": [
        "entity.name.namespace",
        "entity.name.module",
        "entity.name.type.module"
      ],
      "settings": {
        "foreground": "#ee5672"
      }
    },
    {
      "name": "Label",
      "scope": ["variable.other.label", "entity.name.function.decorator"],
      "settings": {
        "foreground": "#047485"
      }
    },
    {
      "name": "Comments Inline",
      "scope": [
        "comment.line.double-slash.cpp",
        "comment.line.triple-slash,cpp",
        "string.comment.cpp",
        "punctuation.definition.comment",
        "source.cpp",
      //  "source.cpp.arduino"
      ],
      "settings": {
        "foreground": "#72b872"
      }
    },
    {
      "name": "Comments Inline Triple-Slash",
      "scope": [
        "comment.line.double-slash.documentation.cpp",
        "punctuation.definition.comment.documentation.cpp"
      ],
      "settings": {
        "foreground": "#cc03ff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "C++ comments block",
      "scope": [
        "comment.block",
        "string.comment",
